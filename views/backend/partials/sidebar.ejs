<div class="sidebar">
    <div class="profile_info">
        <% console.log(admin) %>
        <% if (admin.is_super === '1'){ %>
            <div class="profile_image superadm"></div>
        <% } %>
        <% if(admin.is_super === '0'){ %>
            <div class="profile_image adm"></div>
        <% } %>
        <h3><%= admin.name %> <%= admin.surname %></h3>
        <p><%= admin.email %></p>
    </div>
    <a href="/backend/dashboard" id="dashboard">
        <i class="fas fa-desktop"></i><span>Dashboard</span>
    </a>
    <a href="/backend/manageadmins" id="manageadmins">
        <i class="fas fa-users-cog"></i><span>Manage Admins</span>
    </a>
    <a href="/backend/addadmin" id="addAdmin">
        <i class="fas fa-user-plus"></i><span>Add Admin </span>
    </a>
</div>