    <%- include('partials/sidebar') %>
    <div class="content">
    <form class="login_form" action="/backend/changepass/change" method="post">
        <h1>Change Password</h1>
        <%- include('partials/sessionerrors') %>
        <div class="input-wrapper"><input type="password" class="form-input" placeholder="Current Password" name="currentpass"><i class="fas fa-eye see"></i></div>
        <div class="input-wrapper"><input type="password" class="form-input" placeholder="New Password" name="newpass"><i class="fas fa-eye see"></i></div>
        <div class="input-wrapper"><input type="password" class="form-input" placeholder="Confirm" name="confirmnewpass"><i class="fas fa-eye see"></i></div>
        <input type="submit" class="change" value="Change">
    </form>
    </div>
</div>
